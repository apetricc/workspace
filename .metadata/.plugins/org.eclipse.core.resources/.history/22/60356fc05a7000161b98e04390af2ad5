package drawingpackage;


public class Box extends DrawProgram{

	
	private int x;
	private int y;
	private Box parent;
	private Box child;
	
	public Box(int x, int y, Box parent, Box child) {
		
		this.x = x;
		this.y = y;
		this.parent = parent;
		this.child = child;
	}

	public int getX() {
		return x;
	}

	public void setX(int x) {
		this.x = x;
	}

	public int getY() {
		return y;
	}

	public void setY(int y) {
		this.y = y;
	}

	public Box getParent() {
		return parent;
	}

	public void setParent(Box parent) {
		this.parent = parent;
	}

	public Box getChild() {
		return child;
	}

	public void setChild(Box child) {
		this.child = child;
	}
	
	
	
	
}
