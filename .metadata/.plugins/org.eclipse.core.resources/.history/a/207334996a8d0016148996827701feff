package drawings2;

import csci348.drawings.SimpleDrawing;

public class Arrow extends SimpleDrawProgram {
	private int startX;
	private int startY;
	private int endX;
	private int endY;
	
	public Arrow(int startX, int startY, int endX, int endY) {
		this.startX = startX;
		this.startY = startY;
		this.endX = endX;
		this.endY = endY;
	}
	
	public void draw(SimpleDrawing palette) {
		
	}

	


}
